{
  "extends": "next/core-web-vitals",
  "rules": {
    "react/prop-types": "off",
    "indent": ["warn", 2],
    "no-console": ["warn"],
    "no-unused-vars": "warn",
    "keyword-spacing": ["warn", { "before": true }],
    "linebreak-style": "off",
    "object-curly-spacing": ["warn", "always"],
    "arrow-spacing": "warn"
  }
}
